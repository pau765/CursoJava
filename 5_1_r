int numvalores[] = new int [10];
        for (int i = 0; i < numvalores.length; i++) {
            numvalores[i] = (int) (Math.random() * 100 +1);
        }
        int suma = 0;
        for(int valor : numvalores) {
            suma += valor;
        }
        System.out.println("La suma de los valores aleatorios es " + suma);
          System.out.println("La suma de los valores aleatorios es " + suma);
