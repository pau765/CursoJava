 public static void main(String[] args) {
       int num[] = {4,-18,18,0,-4,25};
        System.out.println("El numero mayor es: " + max(num));
    }

   static int max(int t[]) {
       int max = t [0];
       for (int e: t) {
           if (e > max) {
               max = e;
           }
       }
       return (max);
    }
